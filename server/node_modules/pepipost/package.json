{
  "name": "pepipost",
  "version": "5.0.0",
  "description": "Official nodejs library for sending email using web API v5",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pepipost/pepipost-sdk-nodejs.git"
  },
