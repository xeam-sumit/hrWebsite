    "Bulk Email",
    "API",
    "Email"
  ],
  "author": "Vikram Sahu - DX team, Pepipost",
  "bugs": {
    "email": "dx@pepipost.com"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3"
  },
  "dependencies": {
    "moment": "^2.17.1",
    "request": "^2.55.0"
  },
  "eslintConfig": {
